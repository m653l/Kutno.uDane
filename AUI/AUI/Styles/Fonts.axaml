<Styles xmlns="https://github.com/avaloniaui"

        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Design.PreviewWith>
		<StackPanel>
			<Label FontFamily="{DynamicResource JetBrainsMonoThin}">
				bober
			</Label>
			<Label FontFamily="{DynamicResource JetBrainsMonoLight}">
				bober
			</Label>
			<Label FontFamily="{DynamicResource JetBrainsMonoRegular}">
				bober
			</Label>
			<Label FontFamily="{DynamicResource JetBrainsMonoThin}">
				bober
			</Label>
		</StackPanel>
	</Design.PreviewWith>

	<!-- Add Styles Here -->
	<Style>
		<Style.Resources>
			<FontFamily x:Key="JetBrainsMonoItalic">avares://AUI/Assets/Fonts/JetBrainsMono-Italic.ttf#JetBrains Mono</FontFamily>
			<FontFamily x:Key="JetBrainsMonoLight">avares://AUI/Assets/Fonts/JetBrainsMono-Light.ttf#JetBrains Mono</FontFamily>
			<FontFamily x:Key="JetBrainsMonoRegular">avares://AUI/Assets/Fonts/JetBrainsMono-Regular.ttf#JetBrains Mono</FontFamily>
			<FontFamily x:Key="JetBrainsMonoThin">avares://AUI/Assets/Fonts/JetBrainsMono-Thin.ttf#JetBrains Mono</FontFamily>
		</Style.Resources>
	</Style>
</Styles>
